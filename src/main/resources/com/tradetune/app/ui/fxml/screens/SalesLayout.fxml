<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>


<StackPane fx:id="rootStack" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.tradetune.app.ui.controller.SalesLayoutController">
    <BorderPane prefHeight="720.0" prefWidth="1280.0" styleClass="layout-root"
                stylesheets="@/com/tradetune/app/ui/css/base.css">

        <top>
            <fx:include fx:id="header" source="../components/Header.fxml"/>
        </top>

        <left>
            <fx:include fx:id="nav" source="../components/Nav.fxml"/>
        </left>

        <center>
            <fx:include fx:id="content" source="../components/SectionContent.fxml"/>
        </center>

    </BorderPane>
</StackPane>
